@{
	if (Model == IntranetPOPS1819.Models.StatutLigneDeFrais.EnAttente)
	{
		<font color="DarkOrange">En Attente</font>
	}
	else if (Model == IntranetPOPS1819.Models.StatutLigneDeFrais.Validée)
	{
		<font color="ForestGreen">Validée</font>
	}
	else if (Model == IntranetPOPS1819.Models.StatutLigneDeFrais.ValidéeChef)
	{
		<font color="ForestGreen">Validée par le chef</font>
	}
	else
	{
		<font color="FireBrick">Refusée</font>
	}
}
